{
    "rewrites": [
      { "source": "/ASP/**", "destination": "/ASP/index.html" },
      { "source": "/**", "destination": "/ASP/index.html" }
    ],
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png|svg|webp)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }